{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome against localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3100",
            "webRoot": "${workspaceFolder}/src",
            "sourceMaps": true,
            "skipFiles": ["<node_internals>/**"],
            "preLaunchTask": "start"
        }
    ]
}